{
  "name": "store-builder-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-all-proto": "protoc --plugin=@protobuf-ts --ts_out ./proto -I ./proto -I ../proto ../proto/orders.proto ../proto/shipping.proto ../proto/cart.proto ../proto/money.proto ../proto/order_modules.proto ../proto/payments.proto ../proto/payment_providers.proto ../proto/invoice_items.proto ../proto/address.proto ../proto/countries.proto ../proto/labels.proto ../proto/locations.proto ../proto/clients.proto ../proto/settings.proto ../proto/customers.proto ../proto/template_builder.proto ",
    "generate-all-proto-old": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:proto --ts_out=service=grpc-web:proto -I ../proto -I ../proto ../proto/orders.proto ../proto/shipping.proto ../proto/cart.proto ../proto/money.proto ../proto/order_modules.proto ../proto/payments.proto ../proto/invoice_items.proto ../proto/address.proto ../proto/labels.proto ../proto/locations.proto ../proto/clients.proto ../proto/template_builder.proto "
  },
  "dependencies": {
    "@craftjs/core": "^0.2.5",
    "@craftjs/layers": "^0.2.2",
    "@craftjs/utils": "^0.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@ory/kratos-client": "^1.1.0",
    "@protobuf-ts/grpcweb-transport": "^2.9.4",
    "@protobuf-ts/plugin": "^2.9.4",
    "@protobuf-ts/protoc": "^2.9.4",
    "@reduxjs/toolkit": "^2.2.5",
    "browser-headers": "^0.4.1",
    "grpc-web": "^1.5.0",
    "i18next": "^23.11.5",
    "primeicons": "^6.0.1",
    "primereact": "^10.5.1",
    "protobufjs": "^7.2.6",
    "quill": "^2.0.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.2",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.22.3",
    "react-sortablejs": "^6.1.4",
